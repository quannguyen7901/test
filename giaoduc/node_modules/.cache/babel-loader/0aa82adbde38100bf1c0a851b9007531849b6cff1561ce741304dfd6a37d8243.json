= 0; i < this.datas.length; i++) {\r\n            this.statusSet(i, this.datas[i].status)\r\n        }\r\n        this.selectTr(0)\r\n    },\r\n    methods: {\r\n        /**\r\n         * Mở form sửa cho phần tử đã chọn\r\n         * auther:NgQuan\r\n         * create:05/2023\r\n         */\r\n        openEdit() {\r\n            for (let j = 0; j < this.datas.length; j++) {\r\n                if (this.$refs.table[j].classList.contains(\"table__tr--select\")) {\r\n                    this.onClickDetail(this.datas[j])\r\n                }\r\n            }\r\n        },\r\n        /**\r\n         * Chọn phần tử tiếp theo trong bảng\r\n         * auther:NgQuan\r\n         * create:05/2023\r\n         */\r\n        selectTrNext() {\r\n            for (let j = 0; j < this.datas.length; j++) {\r\n                //Tìm kiếm phần tử đang được chọn\r\n                if (this.$refs.table[j].classList.contains(\"table__tr--select\")) {\r\n                    //Nếu phần tử tiếp theo nhỏ hơn độ dài của mảng\r\n                    //(Ko phải phần tử cuối)\r\n                    if (j + 1 < this.datas.length) {\r\n                        //Chọn phần tử tiếp theo và thoát\r\n                        this.selectTr(j + 1)\r\n                        break\r\n                    }\r\n                    //(Là phần tử cuối)\r\n                    else {\r\n                        //Chọn phần tử đầu tiên\r\n                        this.selectTr(0)\r\n                    }\r\n                }\r\n            }\r\n        },\r\n        /**\r\n         * Chọn phần tử trước đó trong bảng\r\n         * auther:NgQuan\r\n         * create:05/2023\r\n         */\r\n        selectTrPre() {\r\n            for (let j = 0; j < this.datas.length; j++) {\r\n                //Tìm kiếm phần tử đang được chọn\r\n                if (this.$refs.table[j].classList.contains(\"table__tr--select\")) {\r\n                    //Nếu đang chọn phần tử đầu tiên\r\n                    if (j == 0) {\r\n                        //Chọn phần tử cuối cùng\r\n                        this.selectTr(this.datas.length - 1)\r\n                        break\r\n                    }\r\n                    else {\r\n                        //Chọn phần tử trước đó\r\n                        this.selectTr(j - 1)\r\n                    }\r\n                }\r\n            }\r\n        },\r\n        /**\r\n         * Chọn một phần tử trong bảng\r\n         * auther:NgQuan\r\n         * @param {Vị trí của phần tử trong bảng} i \r\n         * create:05/2023\r\n         */\r\n        selectTr(i) {\r\n            for (let j = 0; j < this.datas.length; j++) {\r\n                //Tìm phần tử trong bảng\r\n                if (i == j) {\r\n                    //Nếu phần tử đó chưa được chọn\r\n                    if (!this.$refs.table[j].classList.contains(\"table__tr--select\")) {\r\n                        //Chọn phần tử đó\r\n                        this.$refs.table[j].classList.add(\"table__tr--select\")\r\n                    }\r\n                }\r\n                else {\r\n                    //Nếu không phải phần tử được chọn\r\n                    if (this.$refs.table[j].classList.contains(\"table__tr--select\")) {\r\n                        //Bỏ chọn phần tử\r\n                        this.$refs.table[j].classList.remove(\"table__tr--select\")\r\n                    }\r\n\r\n                }\r\n            }\r\n        },\r\n        /**\r\n         * Set trạng thái của phần tử trong bảng\r\n         * auther:NgQuan\r\n         * @param {Vị trí của phần tử trong bảng} i \r\n         * @param {Trạng thái của phần tử} use\r\n         * create:05/2023 \r\n         */\r\n        statusSet(i, use) {\r\n            let statua=this.$refs.status[i].classList\r\n            let statu=this.$refs.statuslabel[i]\r\n            //Nếu trạng thái là true thì hiển thị là sử dụng\r\n            if (use == true) {\r\n                //Thêm class sử dụng\r\n                statua.add(\"table__status--active\"