is.$refs.table[j].classList.contains(\"table__tr--select\")) {\n          //Nếu phần tử tiếp theo nhỏ hơn độ dài của mảng\n          //(Ko phải phần tử cuối)\n          if (j + 1 < this.datas.length) {\n            //Chọn phần tử tiếp theo và thoát\n            this.selectTr(j + 1);\n            break;\n          }\n          //(Là phần tử cuối)\n          else {\n            //Chọn phần tử đầu tiên\n            this.selectTr(0);\n          }\n        }\n      }\n    },\n    /**\r\n     * Chọn phần tử trước đó trong bảng\r\n     * auther:NgQuan\r\n     * create:05/2023\r\n     */\n    selectTrPre() {\n      for (let j = 0; j < this.datas.length; j++) {\n        //Tìm kiếm phần tử đang được chọn\n        if (this.$refs.table[j].classList.contains(\"table__tr--select\")) {\n          //Nếu đang chọn phần tử đầu tiên\n          if (j == 0) {\n            //Chọn phần tử cuối cùng\n            this.selectTr(this.datas.length - 1);\n            break;\n          } else {\n            //Chọn phần tử trước đó\n            this.selectTr(j - 1);\n          }\n        }\n      }\n    },\n    /**\r\n     * Chọn một phần tử trong bảng\r\n     * auther:NgQuan\r\n     * @param {Vị trí của phần tử trong bảng} i \r\n     * create:05/2023\r\n     */\n    selectTr(i) {\n      for (let j = 0; j < this.datas.length; j++) {\n        //Tìm phần tử trong bảng\n        if (i == j) {\n          //Nếu phần tử đó chưa được chọn\n          if (!this.$refs.table[j].classList.contains(\"table__tr--select\")) {\n            //Chọn phần tử đó\n            this.$refs.table[j].classList.add(\"table__tr--select\");\n          }\n        } else {\n          //Nếu không phải phần tử được chọn\n          if (this.$refs.table[j].classList.contains(\"table__tr--select\")) {\n            //Bỏ chọn phần tử\n            this.$refs.table[j].classList.remove(\"table__tr--select\");\n          }\n        }\n      }\n    },\n    /**\r\n     * Set trạng thái của phần tử trong bảng\r\n     * auther:NgQuan\r\n     * @param {Vị trí của phần tử trong bảng} i \r\n     * @param {Trạng thái của phần tử} use\r\n     * create:05/2023 \r\n     */\n    statusSet(i, use) {\n      //Nếu trạng thái là true thì hiển thị là sử dụng\n      if (use == true) {\n        //Thêm class sử dụng\n        this.$refs.status[i].classList.add(\"table__status--active\");\n        //Set text là sử dụng\n        this.$refs.statuslabel[i].innerText = this.$resource.VN.TextFunction.Use;\n        //Ẩn nút sử dụng\n        this.$refs.use[i].classList.add(\"text--disable\");\n      }\n      //Nếu trạng thái là false thì hiển thị là ngưng sử dụng\n      else {\n        //Nếu trạng thái là sử dụng\n        if (this.$refs.status[i].classList.length > 1) {\n          //Bỏ trạng thái sử dụng\n          this.$refs.status[i].classList.remove(\"table__status--active\");\n        }\n        //Ẩn nút ngưng sử dụng\n        this.$refs.notuse[i].classList.add(\"text--disable\");\n        //Set text là ngưng sử dụng\n        this.$refs.statuslabel[i].innerText = this.$resource.VN.TextFunction.UnUse;\n      }\n    },\n    /**\r\n     * Chuyển đổi trạng thái từ ngừng sử dụng sang sử dụng\r\n     * Auther: Nguyễn Quân\r\n     * @param {Vị trí của phần tử trong bảng} i \r\n    * @param {Trạng thái của phần tử} use\r\n     * Created:05/2023 \r\n     */\n    useItem(i, use) {\n      if ($event)\n        //Nếu trạng thái là ngưng sử dụng\n        if (!use) {\n          //Ẩn nút sử dụng\n          this.$refs.use[i].classList.add(\"text--disable\");\n          //Hiện nút ngưng sử dụng\n          this.$refs.notuse[i].classList.remove(\"text--disable\");\n          //Set trạng thái của phần tử thành sử dụng\n          this.datas[i].status = true;\n          this.statusSet(i, true);\n        }\n        //Nếu trạng thái là sử dụng\n        else {\n          //Hiện nút sử dụng\n          this.$refs.use[i].classList.remove(\"text--disable\");\n          //Ẩn nút ngưng sử dụng\n          this.$refs.notuse[i].classList.add(\"text--disable\");\n          this.datas[i].status = false;\n          //Set trạng thái của phần tử thành ngưng sử dụng\n          this.statusSet(i, false);\n        }\n    },\n    update(a) {\n      this.$emit(\"update:modelValue\", a);\n    },\n    /**\r\n    * Hàm hiện text function con của bảng\r\n    * Auther: Nguyễn Quân\r\n    * Created:05/2023 \r\n    */\n    showFunctionText(i) {\n      const functions = this.$refs.functions__text[i];\n      //Nếu text function chưa hiện thì hiện ra\n      if (functions.style.display == \"none\") {\n        functions.style.display = \"block\";\n      }\n      //Nếu text function hiện rồi thì ẩn đi\n      else {\n        functions.style.display = \"none\";\n      }\n    },\n    /**\r\n    * Hàm hiện function của bảng\r\n    * auther: Nguyễn Quân\r\n    */\n    showFunction(i) {\n      const functions = this.$refs.functions[i];\n      functions.style.left = 'calc(100% + ' + this.scroll + 'px )';\n      functions.style.display = \"flex\";\n    },\n    /**\r\n    * Truyền hàm hiện dailog cảnh báo\r\n    * auther: Nguyễn Quân\r\n    */\n    showDailog() {\n      this.$tnemtter.emit(\"showDailog\");\n    },\n    /**\r\n     * Hàm ẩn function của bảng\r\n     * auther: Nguyễn Quân\r\n     */\n    hideFunction(i) {\n      const functionss = this.$refs.functions__text[i];\n      const functions = this.$refs.functions[i];\n      functions.style.display = \"none\";\n      functionss.style.display = \"none\";\n    },\n    /**\r\n     * Truyền dữ liệu lên from\r\n     * Auther: Nguyễn Quân\r\n     * Created:05/2023 \r\n     */\n    onClickDetail(item) {\n      this.$emit(\"showPopup\", item.id);\n    }\n  }\n};","map":{"version":3,"names":["name","props","modelValue","type","default","scroll","Number","colum","created","$tnemtter","on","selectTrNext","openEdit","selectTrPre","beforeUnmount","update","datas","off","data","hasfuntion","more","$resource","VN","TextTooltip","More","edit","Edit","use","TextFunction","Use","unuse","UnUse","deleted","Delete",