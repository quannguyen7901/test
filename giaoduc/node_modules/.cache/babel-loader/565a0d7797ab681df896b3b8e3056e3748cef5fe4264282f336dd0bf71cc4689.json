tains(\"table__tr--select\")) {\n          if (j == 0) {\n            this.selectTr(this.datas.length - 1);\n            break;\n          } else {\n            this.selectTr(j - 1);\n          }\n        }\n      }\n    },\n    selectTr(i) {\n      for (let j = 0; j < this.datas.length; j++) {\n        if (i == j) {\n          if (!this.$refs.table[j].classList.contains(\"table__tr--select\")) {\n            this.$refs.table[j].classList.add(\"table__tr--select\");\n          }\n        } else {\n          if (this.$refs.table[j].classList.contains(\"table__tr--select\")) {\n            this.$refs.table[j].classList.remove(\"table__tr--select\");\n          }\n        }\n      }\n    },\n    /**\r\n     * Set trạng thái sử dụng hoặc ngừng sử dụng\r\n     * Auther: Nguyễn Quân\r\n     * Created:05/2023 \r\n     */\n    statusSet(i, use) {\n      //Nếu trạng thái là true thì hiể thị là sử dụng\n      if (use == true) {\n        // console.log(this.$refs.statuslabel[i])\n        this.$refs.status[i].classList.add(\"table__status--active\");\n        this.$refs.statuslabel[i].innerText = this.$resource.VN.TextFunction.Use;\n        this.$refs.use[i].classList.add(\"text--disable\");\n      }\n      //Nếu trạng thái là false thì hiển thị là ngưng sử dụng\n      else {\n        if (this.$refs.status[i].classList.length > 1) {\n          this.$refs.status[i].classList.remove(\"table__status--active\");\n        }\n        this.$refs.notuse[i].classList.add(\"text--disable\");\n        this.$refs.statuslabel[i].innerText = this.$resource.VN.TextFunction.UnUse;\n      }\n    },\n    /**\r\n     * Chuyển đổi trạng thái từ ngừng sử dụng sang sử dụng\r\n     * Auther: Nguyễn Quân\r\n     * Created:05/2023 \r\n     */\n    useItem(i, use) {\n      if (use == false) {\n        this.$refs.use[i].classList.add(\"text--disable\");\n        this.$refs.notuse[i].classList.remove(\"text--disable\");\n        this.datas[i].status = true;\n        this.statusSet(i, true);\n      }\n    