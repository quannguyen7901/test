{"ast":null,"code":"// import { keys } from '@/assets/font/fontawesome-pro-6.1.1-web/js/v4-shims'\n// import router from '@/router/router'\n// import MisaTableBodyTr from './MisaTableBodyTr.vue';\n\nexport default {\n  name: \"MisaTableBody\",\n  props: {\n    modelValue: {\n      type: [],\n      default: \"\"\n    },\n    scroll: {\n      type: Number,\n      default: 0\n    },\n    colum: {\n      type: []\n    }\n  },\n  created() {\n    this.$tnemtter.on(\"selectTrNext\", this.selectTrNext);\n    this.$tnemtter.on(\"openEdit\", this.openEdit);\n    this.$tnemtter.on(\"selectTrPre\", this.selectTrPre);\n  },\n  beforeUnmount() {\n    this.$tnemtter.off(\"selectTrNext\");\n    this.$tnemtter.off(\"selectTrPre\");\n  },\n  data() {\n    return {\n      hasfuntion: false,\n      datas: this.modelValue,\n      more: this.$resource.VN.TextTooltip.More,\n      edit: this.$resource.VN.TextTooltip.Edit,\n      use: this.$resource.VN.TextFunction.Use,\n      unuse: this.$resource.VN.TextFunction.UnUse,\n      deleted: this.$resource.VN.TextFunction.Delete,\n      status: \"\"\n    };\n  },\n  mounted() {\n    /**\r\n     * Set trạng thái cho các thành phần trong bảng\r\n     * auther:NgQuan\r\n     * create:05/2023\r\n     */\n    for (let i = 0; i < this.datas.length; i++) {\n      this.statusSet(i, this.datas[i].Status);\n    }\n    this.selectTr(0);\n    console.log(this.modelValue);\n  },\n  methods: {\n    /**\r\n     * Mở form sửa cho phần tử đã chọn\r\n     * auther:NgQuan\r\n     * create:05/2023\r\n     */\n    openEdit() {\n      for (let j = 0; j < this.datas.length; j++) {\n        if (this.$refs.table[j].classList.contains(\"table__tr--select\")) {\n          this.onClickDetail(this.datas[j]);\n        }\n      }\n    },\n    /**\r\n     * Chọn phần tử tiếp theo trong bảng\r\n     * auther:NgQuan\r\n     * create:05/2023\r\n     */\n    selectTrNext() {\n      for (let j = 0; j < this.datas.length; j++) {\n        //Tìm kiếm phần tử đang được chọn\n        if (this.$refs.table[j].classList.contains(\"table__tr--select\")) {\n          //Nếu phần tử tiếp theo nhỏ hơn độ dài của mảng\n          //(Ko phải phần tử cuối)\n          if (j + 1 < this.datas.length) {\n            //Chọn phần tử tiếp theo và thoát\n            this.selectTr(j + 1);\n            break;\n          }\n          //(Là phần tử cuối)\n          else {\n            //Chọn phần tử đầu tiên\n            this.selectTr(0);\n          }\n        }\n      }\n    },\n    /**\r\n     * Chọn phần tử trước đó trong bảng\r\n     * auther:NgQuan\r\n     * create:05/2023\r\n     */\n    selectTrPre() {\n      for (let j = 0; j < this.datas.length; j++) {\n        //Tìm kiếm phần tử đang được chọn\n        if (this.$refs.table[j].classList.contains(\"table__tr--select\")) {\n          //Nếu đang chọn phần tử đầu tiên\n          if (j == 0) {\n            //Chọn phần tử cuối cùng\n            this.selectTr(this.datas.length - 1);\n            break;\n          } else {\n            //Chọn phần tử trước đó\n            this.selectTr(j - 1);\n          }\n        }\n      }\n    },\n    /**\r\n     * Chọn một phần tử trong bảng\r\n     * auther:NgQuan\r\n     * @param {Vị trí của phần tử trong bảng} i \r\n     * create:05/2023\r\n     */\n    selectTr(i) {\n      for (let j = 0; j < this.datas.length; j++) {\n        let table = this.$refs.table[j].classList;\n        //Tìm phần tử trong bảng\n        if (i == j) {\n          //Nếu phần tử đó chưa được chọn\n          if (!table.contains(\"table__tr--select\")) {\n            //Chọn phần tử đó\n            table.add(\"table__tr--select\");\n          }\n        } else {\n          //Nếu không phải phần tử được chọn\n          if (table.contains(\"table__tr--select\")) {\n            //Bỏ chọn phần tử\n            table.remove(\"table__tr--select\");\n          }\n        }\n      }\n    },\n    /**\r\n     * Set trạng thái của phần tử trong bảng\r\n     * auther:NgQuan\r\n     * @param {Vị trí của phần tử trong bảng} i \r\n     * @param {Trạng thái của phần tử} use\r\n     * create:05/2023 \r\n     */\n    statusSet(i, use) {\n      let statua = this.$refs.status[i].classList;\n      let statu = this.$refs.statuslabel[i];\n      //Nếu trạng thái là true thì hiển thị là sử dụng\n      if (use == 1) {\n        //Thêm class sử dụng\n        statua.add(\"table__status--active\");\n        //Set text là sử dụng\n        statu.innerText = this.$resource.VN.TextFunction.Use;\n        //Ẩn nút sử dụng\n        this.$refs.use[i].classList.add(\"text--disable\");\n      }\n      //Nếu trạng thái là false thì hiển thị là ngưng sử dụng\n      else {\n        //Nếu trạng thái là sử dụng\n        if (statua.length > 1) {\n          //Bỏ trạng thái sử dụng\n          statua.remove(\"table__status--active\");\n        }\n        //Ẩn nút ngưng sử dụng\n        this.$refs.notuse[i].classList.add(\"text--disable\");\n        //Set text là ngưng sử dụng\n        statu.innerText = this.$resource.VN.TextFunction.UnUse;\n      }\n    },\n    /**\r\n     * Chuyển đổi trạng thái từ ngừng sử dụng sang sử dụng\r\n     * Auther: Nguyễn Quân\r\n     * @param {Vị trí của phần tử trong bảng} i \r\n    * @param {Trạng thái của phần tử} use\r\n     * Created:05/2023 \r\n     */\n    useItem(i, use) {\n      let uses = this.$refs.use[i].classList;\n      let unuse = this.$refs.notuse[i].classList;\n      //Nếu trạng thái là ngưng sử dụng\n      if (use == 0) {\n        //Ẩn nút sử dụng\n        uses.add(\"text--disable\");\n        //Hiện nút ngưng sử dụng\n        unuse.remove(\"text--disable\");\n        //Set trạng thái của phần tử thành sử dụng\n        this.datas[i].Status = 1;\n        this.statusSet(i, 1);\n        //Update dữ liệu\n        this.update(this.datas);\n      }\n    },\n    /**\r\n    * Chuyển đổi trạng thái từ ngừng sử dụng sang sử dụng\r\n    * Auther: Nguyễn Quân\r\n    * @param {Vị trí của phần tử trong bảng} i \r\n    * @param {Trạng thái của phần tử} use\r\n    * Created:05/2023 \r\n    */\n    unUseItem(i, use) {\n      let uses = this.$refs.use[i].classList;\n      let unuse = this.$refs.notuse[i].classList;\n      //Nếu trạng thái là ngưng sử dụng\n      if (use == 1) {\n        //Ẩn nút sử dụng\n        uses.remove(\"text--disable\");\n        //Hiện nút ngưng sử dụng\n        unuse.add(\"text--disable\");\n        //Set trạng thái của phần tử thành sử dụng\n        this.datas[i].Status = 0;\n        this.statusSet(i, 0);\n        //Update dữ liệu\n        this.update(this.datas);\n      }\n    },\n    /**\r\n     * Hàm cập nhật dữ liệu của v-model\r\n     * auther:NgQuan\r\n     * @param {Dữ liệu cập nhật} a \r\n     * create:05/2023\r\n     */\n    update(a) {\n      // alert(\"Đã update\")\n      this.$emit(\"update:modelValue\", a);\n    },\n    /**\r\n    * Hàm hiện text function con của bảng\r\n    * Auther: Nguyễn Quân\r\n    * Created:05/2023 \r\n    */\n    showFunctionText(i) {\n      const functions = this.$refs.functions__text[i];\n      //Nếu text function chưa hiện thì hiện ra\n      if (functions.style.display == \"none\") {\n        functions.style.display = \"block\";\n      }\n      //Nếu text function hiện rồi thì ẩn đi\n      else {\n        functions.style.display = \"none\";\n      }\n    },\n    /**\r\n    * Hàm hiện function của bảng\r\n    * auther: Nguyễn Quân\r\n    */\n    showFunction(i) {\n      const functions = this.$refs.functions[i];\n      functions.style.left = 'calc(100% + ' + this.scroll + 'px )';\n      functions.style.display = \"flex\";\n    },\n    /**\r\n    * Truyền hàm hiện dailog cảnh báo\r\n    * auther: Nguyễn Quân\r\n    */\n    showDailog() {\n      this.$tnemtter.emit(\"showDailog\");\n    },\n    /**\r\n     * Hàm ẩn function của bảng\r\n     * auther: Nguyễn Quân\r\n     */\n    hideFunction(i) {\n      const functionss = this.$refs.functions__text[i];\n      const functions = this.$refs.functions[i];\n      functions.style.display = \"none\";\n      functionss.style.display = \"none\";\n    },\n    /**\r\n     * Truyền dữ liệu lên from\r\n     * Auther: Nguyễn Quân\r\n     * Created:05/2023 \r\n     */\n    onClickDetail(item) {\n      this.$emit(\"showPopup\", item.EmulatitleId);\n    }\n  }\n};","map":{"version":3,"names":["name","props","modelValue","type","default","scroll","Number","colum","created","$tnemtter","on","selectTrNext","openEdit","selectTrPre","beforeUnmount","off","data","hasfuntion","datas","more","$resource","VN","TextTooltip","More","edit","Edit","use","TextFunction","Use","unuse","UnUse","deleted","Delete","status","mounted","i","length","statusSet","Status","selectTr","console","log","methods","j","$refs","table","classList","contains","onClickDetail","add","remove","statua","statu","statuslabel","innerText","notuse","useItem","uses","update","unUseItem","a","$emit","showFunctionText","functions","functions__text","style","display","showFunction","left","showDailog","emit","hideFunction","functionss","item","EmulatitleId"],"sources":["D:\\Code\\Misa\\vue Web_3\\giaoduc\\src\\components\\MisaTable.vue\\MisaTableBody.vue"],"sourcesContent":["<template>\r\n    <tbody>\r\n        <tr v-for=\"(item, index) in datas\" :key=\"index\" @dblclick=\"onClickDetail(item)\" @click=\"selectTr(index)\"\r\n            @mouseenter=\"showFunction(index)\" @mouseleave=\"hideFunction(index)\" ref=\"table\">\r\n            <td>\r\n                <MisaCheckbox></MisaCheckbox>\r\n            </td>\r\n            <td v-for=\"it in colum\" :key=\"it\">{{ item[it] }} </td>\r\n            <td>\r\n                <div ref=\"status\" class=\"table__status\"></div><label for=\"\" ref=\"statuslabel\"></label>\r\n            </td>\r\n            <div ref=\"functions\" class=\"table__function\">\r\n                <div>\r\n                    <MisaIcon @click=\"onClickDetail(item)\" hastooltip=\"true\" :tooltext=\"edit\" class=\"tooltip--top\"\r\n                        styles=\"icon--24\" icon=\"fa-light fa-pen\"></MisaIcon>\r\n                </div>\r\n                <div @click=\"showFunctionText(index)\">\r\n                    <MisaIcon styles=\"icon--24\" hastooltip=\"true\" :tooltext=\"more\" class=\"tooltip--top\"\r\n                        icon=\"fa-light fa-ellipsis\"></MisaIcon>\r\n                    <div ref=\"functions__text\" class=\"table__function--text\">\r\n                        <div ref=\"use\" @click=\"useItem(index, item.Status)\" class=\"text\">{{ use }}</div>\r\n                        <div ref=\"notuse\" @click=\"unUseItem(index, item.Status)\" class=\"text\">{{ unuse }}</div>\r\n                        <div class=\"red text\" @click=\"showDailog\">{{ deleted }}</div>\r\n                    </div>\r\n                </div>\r\n            </div>\r\n        </tr>\r\n        <!-- <MisaTableBodyTr :colum=\"colum\" :scroll=\"scroll\" @show=\"onClickDetail(item)\" v-for=\"item in datas\" :key=\"item\" @dblclick=\"onClickDetail(item)\" :datas=\"item\"></MisaTableBodyTr> -->\r\n    </tbody>\r\n</template>\r\n\r\n<script>\r\n// import { keys } from '@/assets/font/fontawesome-pro-6.1.1-web/js/v4-shims'\r\n// import router from '@/router/router'\r\n// import MisaTableBodyTr from './MisaTableBodyTr.vue';\r\n\r\nexport default {\r\n    name: \"MisaTableBody\",\r\n    props: {\r\n        modelValue: {\r\n            type: [],\r\n            default: \"\"\r\n        },\r\n        scroll: {\r\n            type: Number,\r\n            default: 0\r\n        },\r\n        colum: {\r\n            type: []\r\n        }\r\n    },\r\n    created() {\r\n        this.$tnemtter.on(\"selectTrNext\", this.selectTrNext)\r\n        this.$tnemtter.on(\"openEdit\", this.openEdit)\r\n        this.$tnemtter.on(\"selectTrPre\", this.selectTrPre)\r\n    },\r\n    beforeUnmount() {\r\n        this.$tnemtter.off(\"selectTrNext\")\r\n        this.$tnemtter.off(\"selectTrPre\")\r\n    },\r\n    data() {\r\n        return {\r\n            hasfuntion: false,\r\n            datas: this.modelValue,\r\n            more: this.$resource.VN.TextTooltip.More,\r\n            edit: this.$resource.VN.TextTooltip.Edit,\r\n            use: this.$resource.VN.TextFunction.Use,\r\n            unuse: this.$resource.VN.TextFunction.UnUse,\r\n            deleted: this.$resource.VN.TextFunction.Delete,\r\n            status: \"\",\r\n        }\r\n    },\r\n    mounted() {\r\n        /**\r\n         * Set trạng thái cho các thành phần trong bảng\r\n         * auther:NgQuan\r\n         * create:05/2023\r\n         */\r\n        for (let i = 0; i < this.datas.length; i++) {\r\n            this.statusSet(i, this.datas[i].Status)\r\n        }\r\n        this.selectTr(0)\r\n        console.log(this.modelValue)\r\n    },\r\n    methods: {\r\n        /**\r\n         * Mở form sửa cho phần tử đã chọn\r\n         * auther:NgQuan\r\n         * create:05/2023\r\n         */\r\n        openEdit() {\r\n            for (let j = 0; j < this.datas.length; j++) {\r\n                if (this.$refs.table[j].classList.contains(\"table__tr--select\")) {\r\n                    this.onClickDetail(this.datas[j])\r\n                }\r\n            }\r\n        },\r\n        /**\r\n         * Chọn phần tử tiếp theo trong bảng\r\n         * auther:NgQuan\r\n         * create:05/2023\r\n         */\r\n        selectTrNext() {\r\n            for (let j = 0; j < this.datas.length; j++) {\r\n                //Tìm kiếm phần tử đang được chọn\r\n                if (this.$refs.table[j].classList.contains(\"table__tr--select\")) {\r\n                    //Nếu phần tử tiếp theo nhỏ hơn độ dài của mảng\r\n                    //(Ko phải phần tử cuối)\r\n                    if (j + 1 < this.datas.length) {\r\n                        //Chọn phần tử tiếp theo và thoát\r\n                        this.selectTr(j + 1)\r\n                        break\r\n                    }\r\n                    //(Là phần tử cuối)\r\n                    else {\r\n                        //Chọn phần tử đầu tiên\r\n                        this.selectTr(0)\r\n                    }\r\n                }\r\n            }\r\n        },\r\n        /**\r\n         * Chọn phần tử trước đó trong bảng\r\n         * auther:NgQuan\r\n         * create:05/2023\r\n         */\r\n        selectTrPre() {\r\n            for (let j = 0; j < this.datas.length; j++) {\r\n                //Tìm kiếm phần tử đang được chọn\r\n                if (this.$refs.table[j].classList.contains(\"table__tr--select\")) {\r\n                    //Nếu đang chọn phần tử đầu tiên\r\n                    if (j == 0) {\r\n                        //Chọn phần tử cuối cùng\r\n                        this.selectTr(this.datas.length - 1)\r\n                        break\r\n                    }\r\n                    else {\r\n                        //Chọn phần tử trước đó\r\n                        this.selectTr(j - 1)\r\n                    }\r\n                }\r\n            }\r\n        },\r\n        /**\r\n         * Chọn một phần tử trong bảng\r\n         * auther:NgQuan\r\n         * @param {Vị trí của phần tử trong bảng} i \r\n         * create:05/2023\r\n         */\r\n        selectTr(i) {\r\n            for (let j = 0; j < this.datas.length; j++) {\r\n                let table=this.$refs.table[j].classList\r\n                //Tìm phần tử trong bảng\r\n                if (i == j) {\r\n                    //Nếu phần tử đó chưa được chọn\r\n                    if (!table.contains(\"table__tr--select\")) {\r\n                        //Chọn phần tử đó\r\n                        table.add(\"table__tr--select\")\r\n                    }\r\n                }\r\n                else {\r\n                    //Nếu không phải phần tử được chọn\r\n                    if (table.contains(\"table__tr--select\")) {\r\n                        //Bỏ chọn phần tử\r\n                        table.remove(\"table__tr--select\")\r\n                    }\r\n\r\n                }\r\n            }\r\n        },\r\n        /**\r\n         * Set trạng thái của phần tử trong bảng\r\n         * auther:NgQuan\r\n         * @param {Vị trí của phần tử trong bảng} i \r\n         * @param {Trạng thái của phần tử} use\r\n         * create:05/2023 \r\n         */\r\n        statusSet(i, use) {\r\n            let statua = this.$refs.status[i].classList\r\n            let statu = this.$refs.statuslabel[i]\r\n            //Nếu trạng thái là true thì hiển thị là sử dụng\r\n            if (use == 1) {\r\n                //Thêm class sử dụng\r\n                statua.add(\"table__status--active\")\r\n                //Set text là sử dụng\r\n                statu.innerText = this.$resource.VN.TextFunction.Use\r\n                //Ẩn nút sử dụng\r\n                this.$refs.use[i].classList.add(\"text--disable\")\r\n            }\r\n            //Nếu trạng thái là false thì hiển thị là ngưng sử dụng\r\n            else {\r\n                //Nếu trạng thái là sử dụng\r\n                if (statua.length > 1) {\r\n                    //Bỏ trạng thái sử dụng\r\n                    statua.remove(\"table__status--active\")\r\n                }\r\n                //Ẩn nút ngưng sử dụng\r\n                this.$refs.notuse[i].classList.add(\"text--disable\")\r\n                //Set text là ngưng sử dụng\r\n                statu.innerText = this.$resource.VN.TextFunction.UnUse\r\n            }\r\n        },\r\n        /**\r\n         * Chuyển đổi trạng thái từ ngừng sử dụng sang sử dụng\r\n         * Auther: Nguyễn Quân\r\n         * @param {Vị trí của phần tử trong bảng} i \r\n        * @param {Trạng thái của phần tử} use\r\n         * Created:05/2023 \r\n         */\r\n        useItem(i, use) {\r\n            let uses = this.$refs.use[i].classList\r\n            let unuse = this.$refs.notuse[i].classList\r\n            //Nếu trạng thái là ngưng sử dụng\r\n            if (use==0) {\r\n                //Ẩn nút sử dụng\r\n                uses.add(\"text--disable\")\r\n                //Hiện nút ngưng sử dụng\r\n                unuse.remove(\"text--disable\")\r\n              