bject.nextElementSibling) {\n        object.removeLastChild();\n      }\n    }\n  },\n  methods: {\n    /**\r\n    * Chưa có api nên làm thủ công bằng mảng\r\n    * Auther: Nguyễn Quân\r\n    * Created:05/2023 \r\n    */\n    halo(a, b) {\n      this.emulation[b] = a;\n    },\n    /**\r\n    * Chưa có api nên làm thủ công bằng mảng\r\n    * Auther: Nguyễn Quân\r\n    * Created:05/2023 \r\n    */\n    saveClick() {\n      console.log(this.emulation);\n    }\n  },\n  data() {\n    return {\n      emulation: [],\n      title: this.$resource.VN.TextFrom.Add,\n      link: \"emulationtitle\",\n      focus: false,\n      name: this.$resource.VN.TextLabel.Name,\n      object: this.$resource.VN.TextLabel.Object,\n      type: this.$resource.VN.TextLabel.Type,\n      level: this.$resource.VN.TextLabel.Level,\n      code: this.$resource.VN.TextLabel.Code,\n      note: this.$resource.VN.TextLabel.Note,\n      batched: this.$resource.VN.TextCheckbox.Batched,\n      frequent: this.$resource.VN.TextCheckbox.Frequent,\n      collective: this.$resource.VN.TextCheckbox.Collective,\n      individual: this.$resource.VN.TextCheckbox.Individual,\n      bacheck: false,\n      frcheck: false,\n      cocheck: false,\n      incheck: false\n    };\n  }\n};","map":{"version":3,"names":["MisaPopupHeader","MisaPopupButton","name","props","id","type","components","created","$tnemtter","on","saveClick","title","$resource","VN","TextFrom","Update","emulation","split","includes","individual","incheck","collective","cocheck","frequent","frcheck","batched","bacheck","focus","beforeUnmount","off","updated","object","$refs","para","document","createElement","classList","add","node","createTextNode","ErrorText","TextNull","appendChild","nextElementSibling","removeLastChild","methods","halo","a","b","console","log","data","Add","link","TextLabel","Name","Object","Type","level","Level","code","Code","note","Note","TextCheckbox","Batched","Frequent","Collective","Individual"],"sources":["D:\\Code\\Misa\\vue 