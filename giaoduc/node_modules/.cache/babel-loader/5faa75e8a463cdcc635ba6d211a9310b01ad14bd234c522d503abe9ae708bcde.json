batched)) {\r\n                this.bacheck = true\r\n            }\r\n        }\r\n        else {\r\n            this.frcheck = true\r\n            this.incheck = true\r\n            //Nếu không truyền dữ liệu thì focus ô đầu tiên\r\n            this.focus = true\r\n        }\r\n    },\r\n    beforeUnmount() {\r\n        this.$tnemtter.off(\"btnSaveOnClick\")\r\n    },\r\n    updated() {\r\n        const object = this.$refs.object\r\n        // kiểm tra input có lỗi không\r\n        if (this.cocheck==false && this.incheck==false) {\r\n            //thêm viền đỏ\r\n            //Thêm thông báo lỗi\r\n                //Tạo thông báo\r\n                const para = document.createElement(\"div\");\r\n                //Gán class thông báo\r\n                para.classList.add(\"input__text--error\");\r\n                //Gán nội dung thông báo\r\n                const node = document.createTextNode(\r\n                    this.object + this.$resource.VN.ErrorText.TextNull);\r\n                para.appendChild(node);\r\n                //Gán thông báo vào dưới input\r\n                object.appendChild(para);\r\n        }\r\n        //Bỏ lỗi input\r\n        else {\r\n            //Bỏ thông báo lỗi dưới check box\r\n            if (object.nextElementSibling) {\r\n                object.removeLastChild();\r\n            }\r\n        }\r\n    },\r\n    methods: {\r\n        /**\r\n        * Chưa có api nên làm thủ công bằng mảng\r\n        * Auther: Nguyễn Quân\r\n        * Created:05/2023 \r\n        */\r\n        halo(a, b) {\r\n            this.emulation[b] = a\r\n        },\r\n        /**\r\n        * Chưa có api nên làm thủ công bằng mảng\r\n        * Auther: Nguyễn Quân\r\n        * Created:05/2023 \r\n        */\r\n        saveClick() {\r\n            console.log(this.emulation)\r\n        }\r\n    },\r\n    data() {\r\n        return {\r\n            emulation: [],\r\n            title: this.$resource.VN.TextFrom.Add,\r\n            link: \"emulationtitle\",\r\n 