 {\r\n                    opa += 0.005;\r\n                    pos += 4;\r\n                    noti.style.right = pos + \"px\";\r\n                    noti.style.opacity = opa;\r\n                }\r\n            }\r\n            setTimeout(this.hideNotification, 3000);\r\n        },\r\n        hideNotification() {\r\n            const noti = document.querySelector(\".content__warning\");\r\n            let id = null;\r\n            let pos = 800;\r\n            let opa = 1;\r\n            clearInterval(id);\r\n            id = setInterval(frame, 5);\r\n            function frame() {\r\n                if (pos == 0) {\r\n                    clearInterval(id);\r\n                    noti.style.display = \"none\";\r\n                } else {\r\n                    opa -= 0.005;\r\n                    pos -= 5;\r\n                    noti.style.right = pos + \"px\";\r\n                    noti.style.opacity = opa;\r\n                }\r\n            }\r\n        },\r\n\r\n        /**\r\n         * Đóng mở slidebar\r\n         */\r\n        btnCollapseOnClick() {\r\n            let icon = document.querySelectorAll(\".sidebar--iconright\");\r\n            let text = document.querySelectorAll(\".sidebar__text\");\r\n            let sidebar = document.querySelector(\".sidebar\");\r\n            try {\r\n                //Đóng slidebar\r\n                if (sidebar.offsetWidth == 200) {\r\n                    //Thu nhỏ slidebar\r\n                    sidebar.style.width = \"73px\";\r\n                    //Ẩn các icon bên phải\r\n                    for (let i = 0; i < icon.length; i++) {\r\n                        icon[i].style.display = \"none\";\r\n                    }\r\n                    //Ẩn các text của slidebar\r\n                    for (let i = 0; i < text.length; i++) {\r\n                        text[i].style.display = \"none\";\r\n                    }\r\n                    //Đổi icon thu phóng\r\n                    document.querySelector(\".sidebar__collapse .icon\").innerHTML = '<i class=\"fa-duotone fa-chevrons-right\"></i>';\r\n                }\r\n                //Mở slidebar\r\n                else {\r\n                    //mở rộng slidebar\r\n                    sidebar.style.width = \"200px\";\r\n                    //Hiện các icon bên phải\r\n                    for (let i = 0; i < icon.length; i++) {\r\n                        icon[i].style.display = \"block\";\r\n                    }\r\n                    //Hiện text\r\n                    for (let i = 0; i < text.length; i++) {\r\n                        text[i].style.display = \"block\";\r\n                    }\r\n                    document.querySelector(\".sidebar__collapse .icon\").innerHTML = '<i class=\"fa-duotone fa-chevrons-left\"></i>';\r\n                }\r\n            } catch (error) {\r\n                console.log(error);\r\n            }\r\n        },\r\n    },\r\n}\r\n</script>\r\n\r\n<style></style>"],"mappings":"AAsFA,eAAe;EACXA,IAAI,EAAE,aAAa;EACnBC,OAAO,EAAE;IACLC,gBAAgBA,CAAA,EAAG;MACf,MAAMC,IAAG,GAAIC,QAAQ,CAACC,aAAa,CAAC,QAAQ,CAAC;MAC7CC,OAAO,CAACC,GAAG,CAACJ,IAAI,CAAC;MACjBA,IAAI,CAACK,KAAK,CAACC,OAAM,GAAI,MAAM;MAC3B,IAAIC,EAAC,GAAI,IAAI;MACb,IAAIC,GAAE,GAAI,CAAC;MACX,IAAIC,GAAE,GAAI,CAAC;MACXC,aAAa,CAACH,EAAE,CAAC;MACjBA,EAAC,GAAII,WAAW,CAACC,KAAK,EAAE,CAAC,CAAC;MAC1B,SAASA,KAAKA,CAAA,EAAG;QACb,IAAIJ,GAAE,IAAK,GAAG,EAAE;UACZE,aAAa,CAACH,EAAE,CAAC;UACjB;QACJ,OAAO;UACHE,GAAE,IAAK,KAAK;UACZD,GAAE,IAAK,CAAC;UACRR,IAAI,CAACK,KAAK,CAACQ,KAAI,GAAIL,GAAE,GAAI,IAAI;UAC7BR,IAAI,CAACK,KAAK,CAACS,OAAM,GAAIL,GAAG;QAC5B;MACJ;MACAM,UAAU,CAAC,IAAI,CAACC,gBAAgB,EAAE,IAAI,CAAC;IAC3C,CAAC;IACDA,gBAAgBA,CAAA,EAAG;MACf,MAAMhB,IAAG,GAAIC,QAAQ,CAACC,aAAa,CAAC,mBAAmB,CAAC;MACxD,IAAIK,EAAC,GAAI,IAAI;MACb,IAAIC,GAAE,GAAI,GAAG;MACb,IAAIC,GAAE,GAAI,CAAC;MACXC,aAAa,CAACH,EAAE,CAAC;MACjBA,EAAC,GAAII,WAAW,CAACC,KAAK,EAAE,CAAC,CAAC;MAC1B,SAASA,KAAKA,CAAA,EAAG;QACb,IAAIJ,GAAE,IAAK,CAAC,EAAE;UACVE,aAAa,CAACH,EAAE,CAAC;UACjBP,IAAI,CAACK,KAAK,CAACC,OAAM,GAA