        id = setInterval(frame, 5);\r\n                function frame() {\r\n                    if (pos == 50.0) {\r\n                        clearInterval(id);\r\n                        // noti.style.display=\"none\";\r\n                    } else {\r\n                        opa += 0.005;\r\n                        pos += 0.25;\r\n                        noti.style.right = pos + \"%\";\r\n                        noti.style.opacity = opa;\r\n                    }\r\n                }\r\n            }\r\n            setTimeout(this.hideNotification, 3000);\r\n        },\r\n        hideNotification() {\r\n            const noti = document.querySelector(\".content__warning\");\r\n            if (noti.style.display == \"flex\") {\r\n                let id = null;\r\n                var pos = 50;\r\n                let opa = 1;\r\n                clearInterval(id);\r\n                id = setInterval(frame, 5);\r\n            }\r\n            function frame() {\r\n                    if (pos == 0) {\r\n                        clearInterval(id);\r\n                        noti.style.display = \"none\";\r\n                    } else {\r\n                        opa -= 0.005;\r\n                        pos -= 0.25;\r\n                        noti.style.right = pos + \"%\";\r\n                        noti.style.opacity = opa;\r\n                    }\r\n                }\r\n\r\n        },\r\n\r\n        /**\r\n         * Đóng mở slidebar\r\n         * auther: Nguyễn Quân\r\n         * created: 05/2023\r\n         */\r\n        btnCollapseOnClick() {\r\n            let icon = document.querySelectorAll(\".sidebar--iconright\");\r\n            let text = document.querySelectorAll(\".sidebar__text\");\r\n            let sidebar = document.querySelector(\".sidebar\");\r\n            try {\r\n                //Đóng slidebar\r\n                if (sidebar.offsetWidth == 200) {\r\n                    //Thu nhỏ slidebar\r\n                    sidebar.style.width = \"73px\";\r\n                    //Ẩn các icon bên phải\r\n                    for (let i = 0; i < icon.length; i++) {\r\n                        icon[i].style.display = \"none\";\r\n                    }\r\n                    //Ẩn các text của slidebar\r\n                    for (let i = 0; i < text.length; i++) {\r\n                        text[i].style.display = \"none\";\r\n                    }\r\n                    //Đổi icon thu phóng\r\n                    document.querySelector(\".sidebar__collapse .icon\").innerHTML = '<i class=\"fa-duotone fa-chevrons-right\"></i>';\r\n                }\r\n                //Mở slidebar\r\n                else {\r\n                    //mở rộng slidebar\r\n                    sidebar.style.width = \"200px\";\r\n                    //Hiện các icon bên phải\r\n                    for (let i = 0; i < icon.length; i++) {\r\n                        icon[i].style.display = \"block\";\r\n                    }\r\n                    //Hiện text\r\n                    for (let i = 0; i < text.length; i++) {\r\n                        text[i].style.display = \"block\";\r\n                    }\r\n                    document.querySelector(\".sidebar__collapse .icon\").innerHTML = '<i class=\"fa-duotone fa-chevrons-left\"></i>';\r\n                }\r\n            } catch (error) {\r\n                console.log(error);\r\n            }\r\n        },\r\n    },\r\n}\r\n</script>\r\n\r\n<style></style>"],"mappings":"AAsFA,eAAe;EACXA,IAAI,EAAE,aAAa;EACnBC,OAAO,EAAE;IACLC,gBAAgBA,CAAA,EAAG;MACf,MAAMC,IAAG,GAAIC,QAAQ,CAACC,aAAa,CAAC,QAAQ,CAAC;MAC7C,IAAIF,IAAI,CAACG,KAAK,CAACC,OAAM,IAAK,MAAM,EAAE;QAC9BJ,IAAI,CAACG,KAAK,CAACC,OAAM,GAAI,MAAM;QAC3B,IAAIC,EAAC,GAAI,IAAI;QACb,IAAIC,GAAE,GAAI,CAAC;QACX,IAAIC,GAAE,GAAI,CAAC;QACXC,aAAa,CAACH,EAAE,CAAC;QACjBA,EAAC,GAAII,WAAW,CAACC,KAAK,EAAE,CAAC,CAAC;QAC1B,SAASA,KAAKA,CAAA,EAAG;UACb,IAAIJ,GAAE,IAAK,IAAI,EAAE;YACbE,aAAa,CAACH,EAAE,CAAC;YACjB;UACJ,OAAO;YACHE,GAAE,IAAK,KAAK;YACZD,GAAE,IAAK,IAAI;YACXN,IAAI,CAACG,KAA