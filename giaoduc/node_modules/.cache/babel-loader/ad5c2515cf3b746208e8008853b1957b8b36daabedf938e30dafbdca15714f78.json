{"ast":null,"code":"import MisaToastMessage from '@/components/MisaToastMessage.vue';\nexport default {\n  name: \"TheContent\",\n  components: {\n    MisaToastMessage\n  },\n  created() {\n    //Hiện dailog cảnh báo khi nhấn nút xóa\n    this.$tnemtter.on(\"showDailog\", () => {\n      this.showdailog = true;\n    });\n    //Ẩn dailog cảnh báo\n    this.$tnemtter.on(\"hideDailog\", () => {\n      this.showdailog = false;\n    });\n  },\n  methods: {\n    /**\r\n     * Hàm hiện ra toast message thông báo\r\n     * auther: Nguyễn Quân\r\n     * created: 05/2023\r\n     */\n    showNotification() {\n      const noti = document.querySelector(\".toast\");\n      let id = null;\n      let pos = 0;\n      let opa = 0;\n      noti.style.display = \"flex\";\n      clearInterval(id);\n      //Lặp lại sau mỗi 5 mili giây\n      id = setInterval(frame, 5);\n      //animation xuất hiện của message\n      function frame() {\n        //Nếu đến vị trí giữa màn hình thì dừng lại\n        if (pos == 50.0) {\n          clearInterval(id);\n          // noti.style.display=\"none\";\n        }\n        //Nếu chưa đến vị trí giữa màn hình thì dịch chuyển thêm\n        else {\n          opa += 0.005;\n          pos += 0.25;\n          noti.style.right = pos + \"%\";\n          noti.style.opacity = opa;\n        }\n      }\n      setTimeout(this.hideNotification, 3000);\n    },\n    /**\r\n    * Hàm ẩn toast message thông báo\r\n    * auther: Nguyễn Quân\r\n    * created: 05/2023\r\n    */\n    hideNotification() {\n      const noti = document.querySelector(\".content__warning\");\n      let id = null;\n      let pos = 50;\n      let opa = 1;\n      clearInterval(id);\n      //Lặp lại sau mỗi 5 mili giây\n      id = setInterval(frame, 5);\n      //animation ẩn đi của message\n      function frame() {\n        //Nếu đến vị trí cuối màn hình thì dừng lại và ẩn đi\n        if (pos == 0) {\n          clearInterval(id);\n          noti.style.display = \"none\";\n        }\n        //Nếu chưa đến vị trí cuối màn hình thì dịch chuyển thêm\n        else {\n          opa -= 0.005;\n          pos -= 0.25;\n          noti.style.right = pos + \"%\";\n          noti.style.opacity = opa;\n        }\n      }\n    }\n  }\n};","map":{"version":3,"names":["MisaToastMessage","name","components","created","$tnemtter","on","showdailog","methods","showNotification","noti","document","querySelector","id","pos","opa","style","display","clearInterval","setInterval","frame","right","opacity","setTimeout","hideNotification"],"sources":["D:\\Code\\Misa\\vue Web_3\\giaoduc\\src\\layout\\TheContent.vue"],"sourcesContent":["<template>\r\n    <div class=\"content\">\r\n        <router-view></router-view>\r\n        <MisaToastMessage></MisaToastMessage>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\nimport MisaToastMessage from '@/components/MisaToastMessage.vue'\r\n\r\nexport default {\r\n    name: \"TheContent\",\r\n    components: {\r\n        MisaToastMessage,\r\n    },\r\n    created(){\r\n        //Hiện dailog cảnh báo khi nhấn nút xóa\r\n        this.$tnemtter.on(\"showDailog\",()=>{this.showdailog=true})\r\n        //Ẩn dailog cảnh báo\r\n        this.$tnemtter.on(\"hideDailog\",()=>{this.showdailog=false})\r\n    },\r\n    methods: {\r\n        /**\r\n         * Hàm hiện ra toast message thông báo\r\n         * auther: Nguyễn Quân\r\n         * created: 05/2023\r\n         */\r\n        showNotification() {\r\n        const noti = document.querySelector(\".toast\");\r\n            let id = null;\r\n            let pos = 0;\r\n            let opa = 0;\r\n            noti.style.display = \"flex\";\r\n            clearInterval(id);\r\n            //Lặp lại sau mỗi 5 mili giây\r\n            id = setInterval(frame, 5);\r\n            //animation xuất hiện của message\r\n            function frame() {\r\n                //Nếu đến vị trí giữa màn hình thì dừng lại\r\n                if (pos == 50.0) {\r\n                    clearInterval(id);\r\n                    // noti.style.display=\"none\";\r\n                }\r\n                //Nếu chưa đến vị trí giữa màn hình thì dịch chuyển thêm\r\n                 else {\r\n                    opa += 0.005;\r\n                    pos += 0.25;\r\n                    noti.style.right = pos + \"%\";\r\n                    noti.style.opacity = opa;\r\n                }\r\n            }\r\n            setTimeout(this.hideNotification, 3000);\r\n        },\r\n         /**\r\n         * Hàm ẩn toast message thông báo\r\n         * auther: Nguyễn Quân\r\n         * created: 05/2023\r\n         */\r\n        hideNotification() {\r\n            const noti = document.querySelector(\".content__warning\");\r\n            let id = null;\r\n            let pos = 50;\r\n            let opa = 1;\r\n            clearInterval(id);\r\n            //Lặp lại sau mỗi 5 mili giây\r\n            id = setInterval(frame, 5);\r\n            //animation ẩn đi của message\r\n            function frame() {\r\n                //Nếu đến vị trí cuối màn hình thì dừng lại và ẩn đi\r\n                if (pos == 0) {\r\n                    clearInterval(id);\r\n                    noti.style.display = \"none\";\r\n                } \r\n                //Nếu chưa đến vị trí cuối màn hình thì dịch chuyển thêm\r\n                else {\r\n                    opa -= 0.005;\r\n                    pos -= 0.25;\r\n                    noti.style.right = pos + \"%\";\r\n                    noti.style.opacity = opa;\r\n                }\r\n            }\r\n        },\r\n\r\n    },\r\n}\r\n</script>\r\n\r\n<style></style>"],"mappings":"AAQA,OAAOA,gBAAe,MAAO,mCAAkC;AAE/D,eAAe;EACXC,IAAI,EAAE,YAAY;EAClBC,UAAU,EAAE;IACRF;EACJ,CAAC;EACDG,OAAOA,CAAA,EAAE;IACL;IACA,IAAI,CAACC,SAAS,CAACC,EAAE,CAAC,YAAY,EAAC,MAAI;MAAC,IAAI,CAACC,UAAU,GAAC,IAAI;IAAA,CAAC;IACzD;IACA,IAAI,CAACF,SAAS,CAACC,EAAE,CAAC,YAAY,EAAC,MAAI;MAAC,IAAI,CAACC,UAAU,GAAC,KAAK;IAAA,CAAC;EAC9D,CAAC;EACDC,OAAO,EAAE;IACL;;;;;IAKAC,gBAAgBA,CAAA,EAAG;MACnB,MAAMC,IAAG,GAAIC,QAAQ,CAACC,aAAa