c thành phần trong bảng\r\n         * auther:NgQuan\r\n         * create:05/2023\r\n         */\r\n        for (let i = 0; i < this.datas.length; i++) {\r\n            this.statusSet(i, this.datas[i].status)\r\n        }\r\n        this.selectTr(0)\r\n    },\r\n    methods: {\r\n        /**\r\n         * Mở form sửa cho phần tử đã chọn\r\n         * auther:NgQuan\r\n         * create:05/2023\r\n         */\r\n        openEdit(){\r\n            for (let j = 0; j < this.datas.length; j++) {\r\n                if (this.$refs.table[j].classList.contains(\"table__tr--select\")) {\r\n                    this.onClickDetail(this.datas[j])\r\n                }\r\n            }\r\n        },\r\n        /**\r\n         * Chọn phần tử tiếp theo trong bảng\r\n         * auther:NgQuan\r\n         * create:05/2023\r\n         */\r\n        selectTrNext() {\r\n            for (let j = 0; j < this.datas.length; j++) {\r\n                //Tìm kiếm phần tử đang được chọn\r\n                if (this.$refs.table[j].classList.contains(\"table__tr--select\")) {\r\n                    //Nếu phần tử tiếp theo nhỏ hơn độ dài của mảng\r\n                    //(Ko phải phần tử cuối)\r\n                    if(j+1<this.datas.length){\r\n                        //Chọn phần tử tiếp theo và thoát\r\n                        this.selectTr(j+1)\r\n                        break\r\n                    }\r\n                    //(Là phần tử cuối)\r\n                    else{\r\n                        //Chọn phần tử đầu tiên\r\n                        this.selectTr(0)\r\n                    }\r\n                }\r\n            }\r\n        },\r\n        /**\r\n         * Chọn phần tử trước đó trong bảng\r\n         */\r\n        selectTrPre() {\r\n            for (let j = 0; j < this.datas.length; j++) {\r\n                //Tìm kiếm phần tử đang được chọn\r\n                if (this.$refs.table[j].classList.contains(\"table__tr--select\")) {\r\n                    //Nếu đang chọn phần tử đầu tiên\r\n 