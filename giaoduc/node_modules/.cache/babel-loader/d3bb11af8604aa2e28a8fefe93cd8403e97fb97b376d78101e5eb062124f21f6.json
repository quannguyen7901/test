{"ast":null,"code":"// import EmulationTitleForm from './EmulationTitleForm.vue';\nimport EmulationTitleTable from './EmulationTitleTable.vue';\nimport EmulationTitleHeader from './EmulationTitleHeader.vue';\nimport EmulationTitleFooter from './EmulationTitleFooter.vue';\nimport MisaDailog from '@/components/MisaDailog.vue';\nexport default {\n  name: \"EmulationTitleHome\",\n  created() {\n    //Hiện dailog cảnh báo khi thao tác\n    this.$tnemtter.on(\"showDailog\", this.showDailog);\n    this.$tnemtter.on(\"keysShort\", this.keysShort);\n    //Ẩn dailog cảnh báo\n    this.$tnemtter.on(\"hideDailog\", () => {\n      this.showdailog = false;\n    });\n    window.addEventListener(\"keydown\", this.keyPress);\n  },\n  beforeUnmount() {\n    this.$tnemtter.off(\"showDailog\");\n    this.$tnemtter.off(\"hideDailog\");\n    this.$tnemtter.off(\"keyShort\");\n  },\n  methods: {\n    keyShort(e) {\n      // console.log(e);\n      if (e.keyCode == 49) {\n        this.$tnemtter.emit('addOnClick');\n      }\n    },\n    /**\r\n     * Hiện dailog cảnh báo khi thao tác\r\n     * Auther: Nguyễn Quân\r\n     * Created:05/2023 \r\n     */\n    showDailog(e) {\n      //Khi đóng popup thì hiện dailog cảnh báo\n      if (e == \"update\") {\n        this.title = this.$resource.VN.TextDailog.Noti;\n        this.type = \"update\";\n        this.showdailog = true;\n        this.button = this.$resource.VN.TextButton.Confirm;\n      }\n      //Khi xóa thì hiện dailog xóa\n      else {\n        this.title = this.$resource.VN.TextDailog.Delete;\n        this.type = \"delete\";\n        this.showdailog = true;\n        this.button = this.$resource.VN.TextButton.Delete;\n      }\n    }\n  },\n  components: {\n    EmulationTitleTable,\n    EmulationTitleHeader,\n    EmulationTitleFooter,\n    MisaDailog\n  },\n  data() {\n    return {\n      title: this.$resource.VN.TextDailog.Delete,\n      button: this.$resource.VN.TextButton.Delete,\n      showdailog: false\n    };\n  }\n};","map":{"version":3,"names":["EmulationTitleTable","EmulationTitleHeader","EmulationTitleFooter","MisaDailog","name","created","$tnemtter","on","showDailog","keysShort","showdailog","window","addEventListener","keyPress","beforeUnmount","off","methods","keyShort","e","keyCode","emit","title","$resource","VN","TextDailog","Noti","type","button","TextButton","Confirm","Delete","components","data"],"sources":["D:\\Code\\Misa\\vue Web_3\\giaoduc\\src\\views\\emulationtitle\\EmulationTitleHome.vue"],"sourcesContent":["<template>\r\n    <div class=\"main\">\r\n        <EmulationTitleHeader></EmulationTitleHeader>\r\n        <EmulationTitleTable></EmulationTitleTable>\r\n        <EmulationTitleFooter></EmulationTitleFooter>\r\n        <router-view name=\"EmulationTitleForm\"></router-view>\r\n    </div>\r\n    <MisaDailog v-if=\"showdailog\" :type=\"type\" :title=\"title\" :button=\"button\"></MisaDailog>\r\n</template>\r\n\r\n<script>\r\n// import EmulationTitleForm from './EmulationTitleForm.vue';\r\nimport EmulationTitleTable from './EmulationTitleTable.vue';\r\nimport EmulationTitleHeader from './EmulationTitleHeader.vue';\r\nimport EmulationTitleFooter from './EmulationTitleFooter.vue';\r\nimport MisaDailog from '@/components/MisaDailog.vue';\r\n\r\nexport default {\r\n    name: \"EmulationTitleHome\",\r\n    created() {\r\n        //Hiện dailog cảnh báo khi thao tác\r\n        this.$tnemtter.on(\"showDailog\", this.showDailog)\r\n        this.$tnemtter.on(\"keysShort\", this.keysShort)\r\n        //Ẩn dailog cảnh báo\r\n        this.$tnemtter.on(\"hideDailog\", () => { this.showdailog = false })\r\n        window.addEventListener(\"keydown\", this.keyPress);\r\n    },\r\n    beforeUnmount() {\r\n        this.$tnemtter.off(\"showDailog\")\r\n        this.$tnemtter.off(\"hideDailog\")\r\n        this.$tnemtter.off(\"keyShort\")\r\n    },\r\n    methods: {\r\n        keyShort(e) {\r\n            // console.log(e);\r\n            if(e.keyCode==49){\r\n                this.$tnemtter.emit('addOnClick')\r\n            }\r\n        },\r\n        /**\r\n         * Hiện dailog cảnh báo khi thao tác\r\n         * Auther: Nguyễn Quân\r\n         * Created:05/2023 \r\n         */\r\n        showDailog(e) {\r\n            //Khi đóng popup thì hiện dailog cảnh báo\r\n            if (e == \"update\") {\r\n                this.title = this.$resource.VN.TextDailog.Noti\r\n                this.type = \"update\"\r\n                this.showdailog = true\r\n                this.button = this.$resource.VN.TextButton.Confirm\r\n            }\r\n            //Khi xóa thì hiện dailog xóa\r\n            else {\r\n                this.title = this.$resource.VN.TextDailog.Delete\r\n                this.type = \"delete\"\r\n                this.showdailog = true\r\n                this.button = this.$resource.VN.TextButton.Delete\r\n            }\r\n        }\r\n    },\r\n    components: {\r\n        EmulationTitleTable, EmulationTitleHeader, EmulationTitleFooter, MisaDailog, \r\n    },\r\n    data() {\r\n        return {\r\n            title: this.$resource.VN.TextDailog.Delete,\r\n            button: this.$resource.VN.TextButton.Delete,\r\n            showdailog: false,\r\n        }\r\n    },\r\n}\r\n</script>\r\n\r\n<style>\r\n.main {\r\n    height: calc(100vh - 56px - 48px);\r\n    width: 100%;\r\n    flex: 1;\r\n    display: flex;\r\n    flex-direction: column;\r\n}\r\n</style>"],"mappings":"AAWA;AACA,OAAOA,mBAAkB,MAAO,2BAA2B;AAC3D,OAAOC,oBAAmB,MAAO,4BAA4B;AAC7D,OAAOC,oBAAmB,MAAO,4BAA4B;AAC7D,OAAOC,UAAS,MAAO,6BAA6B;AAEpD,eAAe;EACXC,IAAI,EAAE,oBAAoB;EAC1BC,OAAOA,CAAA,EAAG;IACN;IACA,IAAI,CAACC,SAAS,CAACC,EAAE,CAAC,YAAY,EAAE,IAAI,CAACC,UAAU;IAC/C,IAAI,CAACF,SAAS,CAACC,EAAE,CAAC,WAAW,EAAE,IAAI,CAACE,SAAS;IAC7C;IACA,IAAI,CAACH,SAAS,CAACC,EAAE,CAAC,YAAY,EAAE,MAAM;MAAE,IAAI,CAACG,UAAS,GAAI,KAAI;IAAE,CAAC;IACjEC,MAAM,CAACC,gBAAgB,CAAC,SAAS,EAAE,IAAI,CAACC,QAAQ,CAAC;EACrD,CAAC;EACDC,aAAaA,CAAA,EAAG;IACZ,IAAI,CAACR,SAAS,CAACS,GAAG,CAAC,YAAY;IAC/B,IAAI,CAACT,SAAS,CAACS,GAAG,CAA