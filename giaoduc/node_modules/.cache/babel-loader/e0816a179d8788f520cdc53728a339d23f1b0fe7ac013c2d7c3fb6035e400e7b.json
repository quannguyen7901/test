{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport MisaPopupVue from '@/components/MisaPopup.vue/MisaPopup.vue';\nimport MisaLoading from '@/components/MisaLoading.vue';\nexport default {\n  name: \"EmulationTitleFrom\",\n  props: {\n    id: {\n      type: String,\n      default: \"\"\n    }\n  },\n  data() {\n    return {\n      baseapi: this.$baseapi,\n      emulation: {},\n      title: this.$resource.VN.TextFrom.Add,\n      table: \"RewardLevel\",\n      focus: false,\n      name: this.$resource.VN.TextLabel.Name,\n      object: this.$resource.VN.TextLabel.Object,\n      type: this.$resource.VN.TextLabel.Type,\n      level: this.$resource.VN.TextLabel.Level,\n      code: this.$resource.VN.TextLabel.Code,\n      note: this.$resource.VN.TextLabel.Note,\n      batched: this.$resource.VN.TextCheckbox.Batched,\n      frequent: this.$resource.VN.TextCheckbox.Frequent,\n      collective: this.$resource.VN.TextCheckbox.Collective,\n      individual: this.$resource.VN.TextCheckbox.Individual,\n      bacheck: false,\n      frcheck: false,\n      cocheck: false,\n      incheck: false,\n      combo: false,\n      popup: false,\n      twobutton: false,\n      save: \"\",\n      use: \"\",\n      unuse: \"\",\n      status: \"\"\n    };\n  },\n  components: {\n    MisaPopupVue,\n    MisaLoading\n  },\n  watch: {\n    'emulation.EmulatitleName': function (e) {\n      let code = e.match(/\\b(\\w)/g);\n      if (code != null) {\n        this.emulation.EmulatitleCode = code.join('').toUpperCase();\n      } else {\n        this.emulation.EmulatitleCode = null;\n      }\n    }\n  },\n  created() {\n    this.$tnemtter.on(\"confirmDailog\", this.btnCloseClick);\n    this.$tnemtter.on(\"btnSaveOnClick\", this.saveClick);\n    this.$axios.get(\"RewardLevels\").then(res => {\n      this.rewardlevels = res.data;\n      this.combo = true;\n    });\n    /**\r\n    * Tùy chọn mở from\r\n    * Auther: Nguyễn Quân\r\n    * Created:05/2023 \r\n    */\n    if (this.id != \"\") {\n      //Nếu truyền dữ liệu thì đổi tên from thành sửa\n      this.title = this.$resource.VN.TextFrom.Update;\n      this.$axios.get(`https://localhost:7263/api/v1/Emulatitles/${this.id}`).then(res => {\n        this.emulation = res.data;\n        this.emulatittleObject(this.emulation.EmulatitleObject);\n        this.emulatittleType(this.emulation.EmulatitleType);\n        this.emulatittleStatus(this.emulation.Status);\n        this.save = this.$resource.VN.TextButton.SaveChange;\n        this.twobutton = false;\n        this.popup = true;\n        this.focus = true;\n      }).catch(res => {\n        this.$tnemtter.emit(\"Noti\", this.$resource.VN.Notification.Error, res.response.data.Message);\n        this.$router.push(`/emulatitle`);\n      });\n    } else {\n      this.emulation.id = \"NewID\";\n      this.frcheck = true;\n      this.incheck = true;\n      //Nếu không truyền dữ liệu thì focus ô đầu tiên\n      this.focus = true;\n      this.popup = true;\n      this.twobutton = true;\n      this.save = this.$resource.VN.TextButton.Save;\n    }\n  },\n  beforeUnmount() {\n    this.$tnemtter.off(\"confirmDailog\");\n    this.$tnemtter.off(\"btnSaveOnClick\");\n  },\n  methods: {\n    checkStatus() {\n      this.emulation.Status = this.status;\n    },\n    btnCloseClick(e) {\n      if (e == \"ok\") {\n        this.$router.push(`/emulatitle`);\n      }\n    },\n    /**\r\n     * Hàm xử lý dữ liệu trạng thái khen thưởng\r\n     * @param {Dữ liệu của đối tượng khen thưởng} i \r\n     * auther: NgQuan\r\n     * created: 05/2023\r\n     */\n    emulatittleStatus(i) {\n      if (i == this.$enum.StatusCode.Use) {\n        this.use = true;\n        this.unuse = false;\n      } else {\n        this.use = false;\n        this.unuse = true;\n      }\n      this.status = i;\n    },\n    /**\r\n     * Hàm xử lý dữ liệu đối tượng khen thưởng\r\n     * @param {Dữ liệu của đối tượng khen thưởng} i \r\n     * auther: NgQuan\r\n     * created: 05/2023\r\n     */\n    emulatittleObject(i) {\n      if (i == this.$enum.EmulatitleCheckbox.Individual) {\n        this.incheck = true;\n      } else if (i == this.$enum.EmulatitleCheckbox.Collective) {\n        this.cocheck = true;\n      } else {\n        this.incheck = true;\n        this.cocheck = true;\n      }\n    },\n    /**\r\n    * Hàm xử lý dữ liệu loại khen thưởng\r\n    * @param {Dữ liệu của loại khen thưởng} i \r\n    * auther: NgQuan\r\n    * created: 05/2023\r\n    */\n    emulatittleType(i) {\n      if (i == this.$enum.EmulatitleCheckbox.Frequent) {\n        this.frcheck = true;\n      } else if (i == this.$enum.EmulatitleCheckbox.Batched) {\n        this.bacheck = true;\n      } else {\n        this.frcheck = true;\n        this.bacheck = true;\n      }\n    },\n    /**\r\n    * Hàm check dữ liệu đối tượng khen thưởng\r\n    * Auther: Nguyễn Quân\r\n    * Created:05/2023 \r\n    */\n    checkObject() {\n      const object = this.$refs.object;\n      // kiểm tra input có lỗi không\n      if (this.cocheck == false && this.incheck == false) {\n        const para = document.createElement(\"div\");\n        //Gán class thông báo\n        para.classList.add(\"input__text--error\");\n        //Gán nội dung thông báo\n        const node = document.createTextNode(this.object + this.$resource.VN.ErrorText.TextNull);\n        para.appendChild(node);\n        //Gán thông báo vào dưới input\n        object.parentElement.appendChild(para);\n      } else {\n        //Gán lại dữ liệu theo điều kiện\n        if (this.cocheck == true && this.incheck == false) {\n          this.emulation.EmulatitleObject = this.$enum.EmulatitleCheckbox.Collective;\n        } else if (this.incheck == true && this.cocheck == false) {\n          this.emulation.EmulatitleObject = this.$enum.EmulatitleCheckbox.Individual;\n        } else {\n          this.emulation.EmulatitleObject = 3;\n        }\n        //Bỏ thông báo lỗi dưới input\n        if (object.nextElementSibling) {\n          object.parentElement.removeChild(object.nextElementSibling);\n        }\n      }\n    },\n    /**\r\n    * Hàm check dữ liệu loại khen thưởng\r\n    * Auther: Nguyễn Quân\r\n    * Created:05/2023 \r\n    */\n    checkType() {\n      const object = this.$refs.type;\n      // kiểm tra input có lỗi không\n      if (this.frcheck == false && this.bacheck == false) {\n        const para = document.createElement(\"div\");\n        //Gán class thông báo\n        para.classList.add(\"input__text--error\");\n        //Gán nội dung thông báo\n        const node = document.createTextNode(this.object + this.$resource.VN.ErrorText.TextNull);\n        para.appendChild(node);\n        //Gán thông báo vào dưới input\n        object.parentElement.appendChild(para);\n      } else {\n        //Gán lại dữ liệu theo điều kiện\n        if (this.frcheck == true && this.bacheck == false) {\n          this.emulation.EmulatitleType = this.$enum.EmulatitleCheckbox.Frequent;\n        } else if (this.bacheck == true && this.frcheck == false) {\n          this.emulation.EmulatitleType = this.$enum.EmulatitleCheckbox.Batched;\n        } else {\n          this.emulation.EmulatitleType = 3;\n        }\n        //Bỏ thông báo lỗi dưới input\n        if (object.nextElementSibling) {\n          object.parentElement.removeChild(object.nextElementSibling);\n        }\n      }\n    },\n    /**\r\n    * Chưa có api nên làm thủ công bằng mảng\r\n    * Auther: Nguyễn Quân\r\n    * Created:05/2023 \r\n    */\n    saveClick() {\n      if (this.id != null) {\n        this.$axios.put(`https://localhost:7263/api/v1/Emulatitles/${this.id}`, this.emulation).then(res => {\n          this.emulation = res.data;\n          this.popup = false;\n          this.$router.push(`/emulatitle`);\n          this.$tnemtter.emit(\"Noti\", this.$resource.VN.Notification.Sucsess, this.$resource.VN.SucsessText.SucsessUpdate);\n          this.$tnemtter.emit(\"reload\");\n        });\n      } else {\n        this.$axios.post(\"https://localhost:7263/api/v1/Emulatitles\", this.emulation).then(res => {\n          this.emulation = res.data;\n          this.popup = false;\n          this.$router.push(`/emulatitle`);\n          this.$tnemtter.emit(\"Noti\", this.$resource.VN.Notification.Sucsess, this.$resource.VN.SucsessText.SucsessInsert);\n          this.$tnemtter.emit(\"reload\");\n        });\n      }\n    }\n  }\n};","map":{"version":3,"names":["MisaPopupVue","MisaLoading","name","props","id","type","String","default","data","baseapi","$baseapi","emulation","title","$resource","VN","TextFrom","Add","table","focus","TextLabel","Name","object","Object","Type","level","Level","code","Code","note","Note","batched","TextCheckbox","Batched","frequent","Frequent","collective","Collective","individual","Individual","bacheck","frcheck","cocheck","incheck","combo","popup","twobutton","save","use","unuse","status","components","watch","emulation.EmulatitleName","e","match","EmulatitleCode","join","toUpperCase","created","$tnemtter","on","btnCloseClick","saveClick","$axios","get","then","res","rewardlevels","Update","emulatittleObject","EmulatitleObject","emulatittleType","EmulatitleType","emulatittleStatus","Status","TextButton","SaveChange","catch","emit","Notification","Error","response","Message","$router","push","Save","beforeUnmount","off","methods","checkStatus","i","$enum","StatusCode","Use","EmulatitleCheckbox","checkObject","$refs","para","document","createElement","classList","add","node","createTextNode","ErrorText","TextNull","appendChild","parentElement","nextElementSibling","removeChild","checkType","put","Sucsess","SucsessText","SucsessUpdate","post","SucsessInsert"],"sources":["D:\\Code\\Misa\\vue Web_3\\giaoduc\\src\\views\\emulationtitle\\EmulationTitleForm.vue"],"sourcesContent":["<template>\r\n    <MisaLoading v-if=\"isloading\"></MisaLoading>\r\n    <MisaPopupVue v-if=\"popup\" :title=\"title\" :twobutton=\"twobutton\" :save=\"save\">\r\n        <div class=\"popup__content\">\r\n            <div class=\"popup__input\">\r\n                <div class=\"popup__item\">\r\n                    <MisaTextBox :focus=\"focus\" v-model=\"emulation.EmulatitleName\" haslabel=\"true\" hasspan=\"true\"\r\n                        :label=\"name\">\r\n                    </MisaTextBox>\r\n                </div>\r\n                <div class=\"popup__item popup__item--50\">\r\n                    <div class=\"popup__item--left\">\r\n                        <MisaTextBox v-model=\"emulation.EmulatitleCode\" haslabel=\"true\" hasspan=\"true\" :label=\"code\">\r\n                        </MisaTextBox>\r\n                    </div>\r\n                    <div class=\"popup__item--right\">\r\n                        <label>{{ object }} </label><span>*</span>\r\n                        <div class=\"popup__checkbox\" @click=\"checkObject\" ref=\"object\">\r\n                            <MisaCheckbox v-model=\"incheck\" :text=\"individual\"></MisaCheckbox>\r\n                            <MisaCheckbox v-model=\"cocheck\" :text=\"collective\"></MisaCheckbox>\r\n                        </div>\r\n                    </div>\r\n                </div>\r\n                <div class=\"popup__item popup__item--50\">\r\n                    <div class=\"popup__item--left\">\r\n                        <MisaCombobox v-if=\"combo\" :combodata=\"rewardlevels\"\r\n                            v-model:combo-value=\"emulation.RewardLevelId\" :nametable=\"table\" hasspan=\"true\" :label=\"level\">\r\n                        </MisaCombobox>\r\n                    </div>\r\n                    <div class=\"popup__item--right\">\r\n                        <label>{{ type }} </label><span>*</span>\r\n                        <div class=\"popup__checkbox\" @click=\"checkType\" ref=\"type\">\r\n                            <MisaCheckbox v-model=\"frcheck\" :text=\"frequent\"></MisaCheckbox>\r\n                            <MisaCheckbox v-model=\"bacheck\" :text=\"batched\"></MisaCheckbox>\r\n                        </div>\r\n                    </div>\r\n                </div>\r\n                <label>{{ note }}</label><br />\r\n                <textarea name=\"\" id=\"\" placeholder=\"Nhập ghi chú\"></textarea>\r\n                <div v-if=\"title==this.$resource.VN.TextFrom.Update\">\r\n                    <label>Trạng thái</label><br />\r\n                    <div class=\"popup__radio\" @click=\"checkStatus\">\r\n                        <MisaRadioButton v-model:status=\"use\" v-model:status-value=\"status\" class=\"popup__input--radio\"\r\n                            name=\"trangthai\" label=\"Sử dụng\" :val=\"this.$enum.StatusCode.Use\"></MisaRadioButton>\r\n                        <MisaRadioButton v-model:status=\"unuse\" v-model:status-value=\"status\" class=\"popup__input--radio\"\r\n                            name=\"trangthai\" label=\"Ngưng sử dụng\" :val=\"this.$enum.StatusCode.UnUse\"></MisaRadioButton>\r\n                    </div>\r\n                </div>\r\n            </div>\r\n        </div>\r\n    </MisaPopupVue>\r\n</template>\r\n\r\n<script>\r\nimport MisaPopupVue from '@/components/MisaPopup.vue/MisaPopup.vue'\r\nimport MisaLoading from '@/components/MisaLoading.vue';\r\nexport default {\r\n    name: \"EmulationTitleFrom\",\r\n    props: {\r\n        id: {\r\n            type: String,\r\n            default: \"\"\r\n        },\r\n    },\r\n    data() {\r\n        return {\r\n            baseapi:this.$baseapi,\r\n            emulation: {},\r\n            title: this.$resource.VN.TextFrom.Add,\r\n            table: \"RewardLevel\",\r\n            focus: false,\r\n            name: this.$resource.VN.TextLabel.Name,\r\n            object: this.$resource.VN.TextLabel.Object,\r\n            type: this.$resource.VN.TextLabel.Type,\r\n            level: this.$resource.VN.TextLabel.Level,\r\n            code: this.$resource.VN.TextLabel.Code,\r\n            note: this.$resource.VN.TextLabel.Note,\r\n            batched: this.$resource.VN.TextCheckbox.Batched,\r\n            frequent: this.$resource.VN.TextCheckbox.Frequent,\r\n            collective: this.$resource.VN.TextCheckbox.Collective,\r\n            individual: this.$resource.VN.TextCheckbox.Individual,\r\n            bacheck: false,\r\n            frcheck: false,\r\n            cocheck: false,\r\n            incheck: false,\r\n            combo: false,\r\n            popup: false,\r\n            twobutton: false,\r\n            save: \"\",\r\n            use: \"\",\r\n            unuse: \"\",\r\n            status: \"\"\r\n        }\r\n    },\r\n    components: {\r\n        MisaPopupVue,MisaLoading\r\n    },\r\n    watch:{\r\n        'emulation.EmulatitleName':function(e){\r\n            let code=e.match(/\\b(\\w)/g)\r\n            if(code !=null ){\r\n                this.emulation.EmulatitleCode=code.join('').toUpperCase()\r\n            }\r\n            else{\r\n                this.emulation.EmulatitleCode=null\r\n            }\r\n        }\r\n    },\r\n    created() {\r\n        this.$tnemtter.on(\"confirmDailog\", this.btnCloseClick)\r\n        this.$tnemtter.on(\"btnSaveOnClick\", this.saveClick)\r\n        this.$axios.get(\"RewardLevels\").then((res) => {\r\n            this.rewardlevels = res.data\r\n            this.combo = true\r\n        })\r\n        /**\r\n        * Tùy chọn mở from\r\n        * Auther: Nguyễn Quân\r\n        * Created:05/2023 \r\n        */\r\n        if (this.id != \"\") {\r\n            //Nếu truyền dữ liệu thì đổi tên from thành sửa\r\n            this.title = this.$resource.VN.TextFrom.Update\r\n            this.$axios.get(`https://localhost:7263/api/v1/Emulatitles/${this.id}`)\r\n                .then((res) => {\r\n                    this.emulation = res.data\r\n                    this.emulatittleObject(this.emulation.EmulatitleObject)\r\n                    this.emulatittleType(this.emulation.EmulatitleType)\r\n                    this.emulatittleStatus(this.emulation.Status)\r\n                    this.save = this.$resource.VN.TextButton.SaveChange\r\n                    this.twobutton = false\r\n                    this.popup = true\r\n                    this.focus = true\r\n                })\r\n                .catch((res) => {\r\n                    this.$tnemtter.emit(\"Noti\", this.$resource.VN.Notification.Error, res.response.data.Message)\r\n                    this.$router.push(`/emulatitle`)\r\n                })\r\n        }\r\n        else {\r\n            this.emulation.id = \"NewID\"\r\n            this.frcheck = true\r\n            this.incheck = true\r\n            //Nếu không truyền dữ liệu thì focus ô đầu tiên\r\n            this.focus = true\r\n            this.popup = true\r\n            this.twobutton = true\r\n            this.save = this.$resource.VN.TextButton.Save\r\n        }\r\n    },\r\n    beforeUnmount() {\r\n        this.$tnemtter.off(\"confirmDailog\")\r\n        this.$tnemtter.off(\"btnSaveOnClick\")\r\n    },\r\n    methods: {\r\n        checkStatus() {\r\n            this.emulation.Status = this.status\r\n        },\r\n        btnCloseClick(e) {\r\n            if (e == \"ok\") {\r\n                this.$router.push(`/emulatitle`)\r\n            }\r\n        },\r\n        /**\r\n         * Hàm xử lý dữ liệu trạng thái khen thưởng\r\n         * @param {Dữ liệu của đối tượng khen thưởng} i \r\n         * auther: NgQuan\r\n         * created: 05/2023\r\n         */\r\n        emulatittleStatus(i) {\r\n            if (i == this.$enum.StatusCode.Use) {\r\n                this.use = true\r\n                this.unuse = false\r\n            }\r\n            else {\r\n                this.use = false\r\n                this.unuse = true\r\n            }\r\n            this.status=i\r\n        },\r\n        /**\r\n         * Hàm xử lý dữ liệu đối tượng khen thưởng\r\n         * @param {Dữ liệu của đối tượng khen thưởng} i \r\n         * auther: NgQuan\r\n         * created: 05/2023\r\n         */\r\n        emulatittleObject(i) {\r\n            if (i == this.$enum.EmulatitleCheckbox.Individual) {\r\n                this.incheck = true\r\n            }\r\n            else if (i == this.$enum.EmulatitleCheckbox.Collective) {\r\n                this.cocheck = true\r\n            }\r\n            else {\r\n                this.incheck = true\r\n                this.cocheck = true\r\n            }\r\n        },\r\n        /**\r\n        * Hàm xử lý dữ liệu loại khen thưởng\r\n        * @param {Dữ liệu của loại khen thưởng} i \r\n        * auther: NgQuan\r\n        * created: 05/2023\r\n        */\r\n        emulatittleType(i) {\r\n            if (i == this.$enum.EmulatitleCheckbox.Frequent) {\r\n                this.frcheck = true\r\n            }\r\n            else if (i == this.$enum.EmulatitleCheckbox.Batched) {\r\n                this.bacheck = true\r\n            }\r\n            else {\r\n                this.frcheck = true\r\n                this.bacheck = true\r\n            }\r\n        },\r\n        /**\r\n      * Hàm check dữ liệu đối tượng khen thưởng\r\n      * Auther: Nguyễn Quân\r\n      * Created:05/2023 \r\n      */\r\n        checkObject() {\r\n            const object = this.$refs.object\r\n            // kiểm tra input có lỗi không\r\n            if (this.cocheck == false && this.incheck == false) {\r\n                const para = document.createElement(\"div\");\r\n                //Gán class thông báo\r\n                para.classList.add(\"input__text--error\");\r\n                //Gán nội dung thông báo\r\n                const node = document.createTextNode(\r\n                    this.object + this.$resource.VN.ErrorText.TextNull);\r\n                para.appendChild(node);\r\n                //Gán thông báo vào dưới input\r\n                object.parentElement.appendChild(para);\r\n            }\r\n            else {\r\n                //Gán lại dữ liệu theo điều kiện\r\n                if (this.cocheck == true && this.incheck == false) {\r\n                    this.emulation.EmulatitleObject = this.$enum.EmulatitleCheckbox.Collective\r\n                }\r\n                else if (this.incheck == true && this.cocheck == false) {\r\n                    this.emulation.EmulatitleObject = this.$enum.EmulatitleCheckbox.Individual\r\n                }\r\n                else {\r\n                    this.emulation.EmulatitleObject = 3\r\n                }\r\n                //Bỏ thông báo lỗi dưới input\r\n                if (object.nextElementSibling) {\r\n                    object.parentElement.removeChild(object.nextElementSibling);\r\n                }\r\n            }\r\n        },\r\n        /**\r\n        * Hàm check dữ liệu loại khen thưởng\r\n        * Auther: Nguyễn Quân\r\n        * Created:05/2023 \r\n        */\r\n        checkType() {\r\n            const object = this.$refs.type\r\n            // kiểm tra input có lỗi không\r\n            if (this.frcheck == false && this.bacheck == false) {\r\n                const para = document.createElement(\"div\");\r\n                //Gán class thông báo\r\n                para.classList.add(\"input__text--error\");\r\n                //Gán nội dung thông báo\r\n                const node = document.createTextNode(\r\n                    this.object + this.$resource.VN.ErrorText.TextNull);\r\n                para.appendChild(node);\r\n                //Gán thông báo vào dưới input\r\n                object.parentElement.appendChild(para);\r\n            }\r\n            else {\r\n                //Gán lại dữ liệu theo điều kiện\r\n                if (this.frcheck == true && this.bacheck == false) {\r\n                    this.emulation.EmulatitleType = this.$enum.EmulatitleCheckbox.Frequent\r\n                }\r\n                else if (this.bacheck == true && this.frcheck == false) {\r\n                    this.emulation.EmulatitleType = this.$enum.EmulatitleCheckbox.Batched\r\n                }\r\n                else {\r\n                    this.emulation.EmulatitleType = 3\r\n                }\r\n                //Bỏ