 <div class=\"popup__cover\">\r\n        <div class=\"popup\">\r\n            <MisaPopupHeader :title=\"title\" :link=\"link\" hasquestion=\"true\"></MisaPopupHeader>\r\n            <div class=\"popup__content\">\r\n                <div class=\"popup__input\">\r\n                    <div class=\"popup__item\">\r\n                        <MisaTextBox :focus=\"focus\" v-model=\"emulation[0]\" haslabel=\"true\" hasspan=\"true\" :label=\"name\">\r\n                        </MisaTextBox>\r\n                    </div>\r\n                    <div class=\"popup__item popup__item--50\">\r\n                        <div class=\"popup__item--left\">\r\n                            <MisaTextBox v-model=\"emulation.1\" num=\"1\" haslabel=\"true\" hasspan=\"true\" :label=\"code\">\r\n                            </MisaTextBox>\r\n                        </div>\r\n                        <div class=\"popup__item--right\">\r\n                            <label>{{ object }} </label><span>*</span>\r\n                            <div class=\"popup__checkbox\" @click=\"checkObject\" ref=\"object\">\r\n                                <MisaCheckbox v-model=\"incheck\" :text=\"individual\"></MisaCheckbox>\r\n                                <MisaCheckbox v-model=\"cocheck\" :text=\"collective\"></MisaCheckbox>\r\n                            </div>\r\n                        </div>\r\n                    </div>\r\n                    <div class=\"popup__item popup__item--50\">\r\n                        <div class=\"popup__item--left\">\r\n                            <MisaTextBox v-model=\"emulation[3]\" num=\"3\" haslabel=\"true\" hasspan=\"true\" :label=\"level\">\r\n                            </MisaTextBox>\r\n                        </div>\r\n                        <div class=\"popup__item--right\">\r\n                            <label>{{ type }} </label><span>*</span>\r\n                            <div class=\"popup__checkbox\" @click=\"checkType\" ref=\"type\">\r\n                                <MisaCheckbox v-model=\"frcheck\" :text=\"frequent\"></MisaCheckbox>\r\n                                <MisaCheckbox v-model=\"bacheck\" :text=\"batched\"></MisaCheckbox>\r\n                            </div>\r\n                        </div>\r\n                    </div>\r\n                    <label>{{ note }}</label><br />\r\n                    <textarea name=\"\" id=\"\" placeholder=\"Nhập ghi chú\"></textarea>\r\n                </div>\r\n            </div>\r\n            <MisaPopupButton twobutton=\"false\" :linkclose=\"link\"></MisaPopupButton>\r\n        </div>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\nimport MisaPopupHeader from '@/components/MisaPopup.vue/MisaPopupHeader.vue';\r\nimport MisaPopupButton from '@/components/MisaPopup.vue/MisaPopupButton.vue';\r\n\r\nexport default {\r\n    name: \"EmulationTitleFrom\",\r\n    props: {\r\n        id: {\r\n            type: [],\r\n        }\r\n    },\r\n    components: {\r\n        MisaPopupButton, MisaPopupHeader\r\n    },\r\n    created() {\r\n        this.$tnemtter.on(\"btnSaveOnClick\", this.saveClick)\r\n        /**\r\n        * Tùy chọn mở from\r\n        * Auther: Nguyễn Quân\r\n        * Created:05/2023 \r\n        */\r\n        if (this.id != null) {\r\n            //Nếu truyền dữ liệu thì đổi tên from thành sửa\r\n            this.title = this.$resource.VN.TextFrom.Update\r\n            this.emulation = this.id.split(',')\r\n            //Đánh dấu checkbox theo dữ liệu\r\n            if (this.emulation[2].includes(this.individual)) {\r\n                this.incheck = true\r\n            }\r\n            if (this.emulation[2].includes(this.collective)) {\r\n                this.cocheck = true\r\n            }\r\n            if (this.emulation[4].includes(this.frequent)) {\r\n                this.frcheck = true\r\n            }\r\n            if (this.emulation[4].includes(this.batched)) {\r\n                this.bacheck = true\r\n            }\r\n        }\r\n        else {\r\n            this.frcheck = true\r\n            this.incheck = true\r\n            //Nếu không truyền dữ liệu thì focus ô đầu tiên\r\n            this.focus = true\r\n        }\r\n    },\r\n    beforeUnmount() {\r\n        this.$tnemtter.off(\"btnSaveOnClick\")\r\n    },\r\n    methods: {\r\n        /**\r\n      * Hàm check dữ liệu đối tượng khen thưởng\r\n      * Auther: Nguyễn Quân\r\n      * Created:05/2023 \r\n      */\r\n        checkObject() {\r\n            const object = this.$refs.object\r\n            // kiểm tra input có lỗi không\r\n            if (this.cocheck == false && this.incheck == false) {\r\n                const para = document.createElement(\"div\");\r\n                //Gán class thông báo\r\n                para.classList.add(\"input__text--error\");\r\n                //Gán nội dung thông báo\r\n                const node = document.createTextNode(\r\n                    this.object + this.$resource.VN.ErrorText.TextNull);\r\n                para.appendChild(node);\r\n                //Gán thông báo vào dưới input\r\n                object.parentElement.appendChild(para);\r\n            }\r\n            else {\r\n                //Gán lại dữ liệu theo điều kiện\r\n                if(this.cocheck == true && this.incheck == false){\r\n                    this.emulation[2]=this.collective\r\n                }\r\n                else if(this.incheck == true && this.cocheck == false){\r\n                    this.emulation[2]=this.individual\r\n                }\r\n                else{\r\n                    this.emulation[2]=this.individual + \", \" + this.collective \r\n                }\r\n                //Bỏ thông báo lỗi dưới input\r\n                if (object.nextElementSibling) {\r\n                    object.parentElement.removeChild(object.nextElementSibling);\r\n                }\r\n            }\r\n        },\r\n           /**\r\n      * Hàm check dữ liệu loại khen thưởng\r\n      * Auther: Nguyễn Quân\r\n      * Created:05/2023 \r\n      */\r\n      checkType() {\r\n            const object = this.$refs.type\r\n            // kiểm tra input có lỗi không\r\n            if (this.frcheck == false && this.bacheck == false) {\r\n                const para = document.createElement(\"div\");\r\n                //Gán class thông báo\r\n                para.classList.add(\"input__text--error\");\r\n                //Gán nội dung thông báo\r\n                const node = document.createTextNode(\r\n                    this.object + this.$resource.VN.ErrorText.TextNull);\r\n                para.appendChild(node);\r\n                //Gán thông báo vào dưới input\r\n                object.parentElement.appendChild(para);\r\n                this.emulation[4]=this.frequent+' '+this.batched\r\n            }\r\n            else {\r\n                //Gán lại dữ liệu theo điều kiện\r\n                if(this.frcheck == true && this.bacheck == false){\r\n                    this.emulation[4]=this.frequent\r\n                }\r\n                else if(this.bacheck == true && this.frcheck == false){\r\n                    this.emulation[2]=this.batched\r\n                }\r\n                else{\r\n                    this.emulation[2]=this.frequent + \", \" + this.batched \r\n                }\r\n                //Bỏ thông báo lỗi dưới input\r\n                if (object.nextElementSibling) {\r\n                    object.parentElement.removeChild(object.nextElementSibling);\r\n                }\r\n            }\r\n        },\r\n        /**\r\n        * Chưa có api nên làm thủ công bằng mảng\r\n        * Auther: Nguyễn Quân\r\n        * Created:05/2023 \r\n        */\r\n        saveClick() {\r\n            console.log(this.emulation)\r\n        }\r\n    },\r\n    data() {\r\n        return {\r\n            emulation: {},\r\n            title: this.$resource.VN.TextFrom.Add,\r\n            link: \"emulationtitle\",\r\n            focus: false,\r\n            name: this.$resource.VN.TextLabel.Name,\r\n            object: this.$resource.VN.TextLabel.Object,\r\n            type: this.$resource.VN.TextLabel.Type,\r\n            level: this.$resource.VN.TextLabel.Level,\r\n            code: this.$resource.VN.TextLabel.Code,\r\n            note: this.$resource.VN.TextLabel.Note,\r\n            batched: this.$resource.VN.TextCheckbox.Batched,\r\n            frequent: this.$resource.VN.TextCheckbox.Frequent,\r\n            collective: this.$resource.VN.TextCheckbox.Collective,\r\n            individual: this.$resource.VN.TextCheckbox.Individual,\r\n            bacheck: false,\r\n            frcheck: false,\r\n            cocheck: false,\r\n            incheck: false,\r\n        }\r\n    },\r\n}\r\n</script>\r\n\r\n<style></style>"],"mappings":";;EACSA,KAAK,EAAC;AAAc;;EAChBA,KAAK,EAAC;AAAO;;EAETA,KAAK,EAAC;AAAgB;;EAClBA,KAAK,EAAC;AAAc;;EAChBA,KAAK,EAAC;AAAa;;EAInBA,KAAK,EAAC;AAA6B;;EAC/BA,KAAK,EAAC;AAAmB;;EAIzBA,KAAK,EAAC;AAAoB;gCACCC,mBAAA,CAAc,cAAR,GAAC;;EAOtCD,KAAK,EAAC;AAA6B;;EAC/BA,KAAK,EAAC;AAAmB;;EAIzBA,KAAK,EAAC;AAAoB;iCACDC,mBAAA,CAAc,cAAR,GAAC;iCAOhBA,mBAAA,CAAM;iCAC/BA,mBAAA,CAA8D;EAApDC,IAAI,EAAC,EAAE;EAACC,EAAE,EAAC,EAAE;EAACC,WAAW,EAAC;;;;;;;;uBApCpDC,mBAAA,CAyCM,OAzCNC,UAyCM,GAxCFL,mBAAA,CAuCM,OAvCNM,UAuCM,GAtCFC,YAAA,CAAkFC,0BAAA;IAAhEC,KAAK,EAAEC,KAAA,CAAAD,KAAK;IAAGE,IAAI,EAAED,KAAA,CAAAC,IAAI;IAAEC,WAAW,EAAC;8CACzDZ,mBAAA,CAmCM,OAnCNa,UAmCM,GAlCFb,mBAAA,CAiCM,OAjCNc,UAiCM,GAhCFd,mBAAA,CAGM,OAHNe,UAGM,GAFFR,YAAA,CACcS,sBAAA;IADAC,KAAK,EAAEP,KAAA,CAAAO,KAAK;gBAAWP,KAAA,CAAAQ,SAAS;+DAATR,KAAA,CAAAQ,SAAS,MAAAC,MAAA;IAAKC,QAAQ,EAAC,MAAM;IAACC,OAAO,EAAC,MAAM;IAAEC,KAAK,EAAEZ,KAAA,CAAAT;+DAG9FD,mBAAA,CAYM,OAZNuB,UAYM,GAXFvB,mBAAA,CAGM,OAHNwB,UAGM,GAFFjB,YAAA,CACcS,sBAAA;IADqBS,GAAG,EAAC,GAAG;IAACL,QAAQ,EAAC,MAAM;IAACC,OAAO,EAAC,MAAM;IAAEC,KAAK,EAAEZ,KAAA,CAAAgB;wCAGtF1B,mBAAA,CAMM,OANN2B,UAMM,GALF3B,mBAAA,CAA4B,eAAA4B,gBAAA,CAAlBlB,KAAA,CAAAmB,MAAM,kBAAYC,UAAc,EAC1C9B,mBAAA,CAGM;IAHDD,KAAK,EAAC,iBAAiB;IAAEgC,OAAK,EAAAC,MAAA,QAAAA,MAAA,UAAAC,IAAA,KAAEC,QAAA,CAAAC,WAAA,IAA